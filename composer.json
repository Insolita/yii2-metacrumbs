{
    "name": "insolita/yii2-metacrumbs",
    "description": "alternative way for work with breadcrumbs and meta ",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "version": "1.0",
    "authors": [
        {
            "name": "insolita",
            "email": "webmaster100500@ya.ru"
        }
    ],
    "config":            {
        "process-timeout":           1800 ,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    } ,
    "require": {
        "php":">=5.6",
        "yiisoft/yii2": "~2.0.13"
    },
    "require-dev": {
        "vlucas/phpdotenv": "*",
        "codeception/codeception": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3"
    },
    "suggest": {},
    "autoload": {
        "psr-4": {
            "insolita\\metacrumbs\\": "src"
        }
    }
}
